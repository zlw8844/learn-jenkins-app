{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "024848466246.dkr.ecr.us-east-1.amazonaws.com/learnjenkinsapp:#APP_VERSION#",
            "imagePullPolicy": "Always",
            "portMappings": [{
                "name": "ngnix-80-tcp",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }], 
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::024848466246:role/ecsTaskExecutionRole"
}